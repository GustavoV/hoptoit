/* Palette!!!! http://www.colourlovers.com/palette/944213/forever_lost */

$darkpurple:rgba(93,65,87,1);
$grey:rgba(131,134,137,1);
$seafoam:rgba(168,202,186,1);
$green:rgba(202,215,178,1);
$yellow:rgba(235,227,170,1);


body{
  background: url(<%= asset_path 'bgnoise_lg.png' %>);
	/*background-color: $yellow;*/
  color: $darkpurple;
}

.navbar{
  background: $green;
  box-shadow: 0 5px $seafoam;
  opacity: 0.8;
  border:0;
  border-radius: 0;
}

.navbar-default .navbar-nav li a{
  color:$darkpurple;
}

a {
  color:$darkpurple;
}

.irsInfo {
  background: white;
  border: 2px solid $seafoam;
  border-radius: 20px;
} 
.container {
  background:rgba(255,255,255,0.7);
  border-radius: 20px;
}

.logoDiv {
  overflow:hidden;
  
}